generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{

  id          String    @id @default(uuid())
  name        String 
  cpf         String
  birthdate   String
  phone       String?
  email       String    @unique
  password    String

  @@map("user") 

}